#parse("header.vm")
<div class="container">
    <div class="panel">
        <div class="panel-heading">
            <h2>Upload a file:</h2>
        </div>
        <div class="panel-body">
            <form method="post" enctype="multipart/form-data" action="multiupload">
                <div class="well">
                    <div class="row">
                        <div class="col-lg-6">
                            <input type="text" class="form-control" id="inputNumber" name="number" placeholder="#">
                        </div>
                        <div class="col-lg-6">
                            <input type="file" name="file">
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="well">
                        <button class="btn btn-block btn-default"><span class="glyphicon glyphicon-plus"></span>
                        </button>
                    </div>
                    <div class="panel-footer">
                        <button type="submit" class="btn btn-primary btn-lg btn-block">Upload</button>
                    </div>
                </div>
            </form>
        </div>
#parse("footer.vm")